<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="process" extends="default" namespace="/">
        <action name="preProcessManage" class="action.ProcessAction" method="preProcessManage">
            <result name="success">/process_management.jsp</result>
        </action>
        <action name="getProcessList" class="action.ProcessAction" method="getProcessList">
        </action>
        <action name="addProcess" class="action.ProcessAction" method="addProcess">
        </action>
        <action name="updateProcess" class="action.ProcessAction" method="updateProcess">
        </action>
        <action name="updatePImage" class="action.ProcessAction" method="updatePImage">
        </action>
        <action name="updatePVideo" class="action.ProcessAction" method="updatePVideo">
        </action>
        <action name="deleteProcess" class="action.ProcessAction" method="deleteProcess">
        </action>
        <action name="getProcessInfo" class="action.ProcessAction" method="getProcessInfo">
        </action>
    </package>
</struts>